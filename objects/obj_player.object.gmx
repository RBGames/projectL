}

if((down &amp;&amp; y &lt; room_height - 32) &amp;&amp; !colBottom){
    y += intSpeed;
}

/// Shooting
if (canFire){
    // Draw Cursor
    if (!instance_exists(obj_cursor)){
        instance_create(mouse_x,mouse_y,obj_cursor);
    }
    // Firing
    if (instance_number(obj_shoot) &lt; shoots){
        if (action){
            instance_create(x,y,obj_shoot);
        }
    }
}
